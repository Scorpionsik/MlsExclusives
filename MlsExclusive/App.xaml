<Application x:Class="MlsExclusive.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:MlsExclusive"
             StartupUri="Views/MainView.xaml">
    <Application.Resources>
        <SolidColorBrush x:Key="MlsLightGreen" Color="#CEE6C1"/>
        <SolidColorBrush x:Key="MlsGreen">#71BC78</SolidColorBrush>
        <SolidColorBrush x:Key="MlsDarkGreen">#355823</SolidColorBrush>
        <Style TargetType="ListBox">
            <Setter Property="BorderThickness" Value="0"/>
        </Style>
    </Application.Resources>
</Application>
